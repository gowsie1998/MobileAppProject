<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animationCache="true"
    android:background="#00E91E63"
    android:backgroundTint="#009C27B0"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <ImageView
        android:id="@+id/background"
        android:layout_width="755dp"
        android:layout_height="971dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background"
        android:contentDescription="@string/todo6" />

    <Button
        android:id="@+id/goToPark"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/amiko"
        android:text="@string/parkmain"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToSessions"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="150dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/amiko"
        android:text="@string/sessionsmain"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToMap"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/amiko"
        android:text="@string/mapsmain"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToAccount"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="350dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/amiko"
        android:text="@string/accountmain"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToInfo"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="450dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/amiko"
        android:text="@string/informationmain"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/parkicon"
        android:layout_width="180dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/goToPark"
        app:layout_constraintEnd_toStartOf="@+id/goToPark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/goToPark"
        app:srcCompat="@drawable/cariconnew"
        android:contentDescription="@string/todo10" />

    <ImageView
        android:id="@+id/sessionicon"
        android:layout_width="110dp"
        android:layout_height="216dp"
        app:layout_constraintBottom_toBottomOf="@+id/goToSessions"
        app:layout_constraintEnd_toStartOf="@+id/goToSessions"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/goToSessions"
        app:srcCompat="@drawable/sessionsicon"
        android:contentDescription="@string/todo11" />

    <ImageView
        android:id="@+id/mapicon"
        android:layout_width="120dp"
        android:layout_height="216dp"
        app:layout_constraintBottom_toBottomOf="@+id/goToMap"
        app:layout_constraintEnd_toStartOf="@+id/goToMap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/goToMap"
        app:srcCompat="@drawable/mapicon"
        android:contentDescription="@string/todo12" />

    <ImageView
        android:id="@+id/accounticon"
        android:layout_width="110dp"
        android:layout_height="216dp"
        app:layout_constraintBottom_toBottomOf="@+id/goToAccount"
        app:layout_constraintEnd_toStartOf="@+id/goToAccount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/goToAccount"
        app:srcCompat="@drawable/accounticon"
        android:contentDescription="@string/todo13" />

    <ImageView
        android:id="@+id/infoicon"
        android:layout_width="120dp"
        android:layout_height="216dp"
        app:layout_constraintBottom_toBottomOf="@+id/goToInfo"
        app:layout_constraintEnd_toStartOf="@+id/goToInfo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/goToInfo"
        app:srcCompat="@drawable/infoicon"
        android:contentDescription="@string/todo7" />

    <ImageView
        android:id="@+id/warwicklogo"
        android:layout_width="284dp"
        android:layout_height="149dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/warwicklogo"
        android:contentDescription="@string/todo14" />

</androidx.constraintlayout.widget.ConstraintLayout>
